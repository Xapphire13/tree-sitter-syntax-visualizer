{
  "name": "tree-sitter-syntax-visualizer",
  "main": "./lib/tree-sitter-syntax-visualizer-package.ts",
  "version": "1.0.0",
  "description": "Syntax tree visualizer for tree-sitter",
  "keywords": [
    "tree-sitter",
    "syntax",
    "tree",
    "visualizer",
    "compiler",
    "parser"
  ],
  "activationCommands": {
    "atom-workspace": "tree-sitter-syntax-visualizer:toggle"
  },
  "repository": "https://github.com/xapphire13/tree-sitter-syntax-visualizer",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-ide-base": "^3.1.1",
    "atom-ts-transpiler": "^1.5.3",
    "debounce": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.4"
  },
  "atomTranspilers": [
    {
      "transpiler": "atom-ts-transpiler",
      "glob": "/lib/**/*.ts?(x)"
    }
  ],
  "devDependencies": {
    "@types/atom": "^1.40.10",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "tree-sitter": "^0.19.0"
  }
}
